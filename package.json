{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio",
  "repository": "git@github.com:pocka/portfolio.git",
  "author": "pocka <pockawoooh@gmail.com>",
  "private": true,
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 ChromeAndroid versions",
    "last 2 Opera versions",
    "last 2 iOS versions",
    "last 2 Safari versions"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "corejs": 3,
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {},
      "cssnano": {}
    }
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "rm -rf dist && NODE_ENV=production webpack --mode production",
    "deploy": "s3-deploy './dist/**' --cwd './dist/' --bucket $AWS_BUCKET_NAME --distId $AWS_CF_DIST_ID --invalidate '/*' --gzip --etag --deleteRemoved",
    "test": "exit 0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prerender-loader": "^1.3.0",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "s3-deploy": "^1.4.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-ext-html-webpack-plugin": "^4.1.2",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "typescript": "^3.5.3",
    "uglify-template-string-loader": "^1.1.1",
    "webp-loader": "^0.5.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
